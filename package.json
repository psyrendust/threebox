{
  "name": "threebox",
  "version": "0.1.5",
  "description": "A Mapbox GL JS plugin that combines the power of the Three.js 3D library with Mapbox geospatial tools.",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterqliu/threebox.git"
  },
  "author": "@peterqliu and @kronick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peterqliu/threebox/issues"
  },
  "scripts": {
    "build": "rm -rf ./dist && npm run build-main && npm run build-flocking",
    "build-main": "webpack --config webpack.config.main.js --progress",
    "build-flocking": "webpack --config webpack.config.flocking.js --progress",
    "dev": "npm run dev-main & npm run dev-flocking",
    "dev-main": "webpack --watch --config webpack.config.main.js --progress",
    "dev-flocking": "webpack --watch --config webpack.config.flocking.js --progress",
    "start": "npm run dev & node server.js"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "compression-webpack-plugin": "^1.0.0",
    "css-loader": "^0.28.4",
    "dat.gui": "^0.7.0",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.29.0",
    "image-webpack-loader": "^4.0.0",
    "live-server": "^1.2.0",
    "node-sass": "^4.5.3",
    "react-dev-utils": "^4.2.1",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "three": "^0.89.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-glsl-loader": "^1.0.1"
  },
  "peerDependencies": {
    "three": "^0.89.0"
  },
  "dependencies": {
    "@turf/along": "^5.1.5",
    "@turf/destination": "^5.1.5",
    "@turf/distance": "^5.1.5",
    "@turf/helpers": "^5.0.4",
    "gpucomputationrender-threejs": "^1.0.0"
  }
}
